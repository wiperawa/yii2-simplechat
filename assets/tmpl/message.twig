<div class="media {{ settings.itemCssClass }}" data-key="{{ key }}">
    <a class="pull-left" href="#">
         <img class="media-object"   alt="{{  sender.name }}" data-contact = "{{ sender.id }}" style="width: 32px; height: 32px;" src="{{ sender.avatar }}"/> 
    </a>
    <div class="media-body">
        <small class="pull-right time"><i class="fa fa-clock-o"></i> {{ model.date }}</small>
        <h5 class="media-heading">{{user.id == sender.id ? 'You' : sender.name }}</h5>
        <small class="col-lg-10">{{ model.text|raw }}</small>
    </div>
</div>
